___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const log = require('logToConsole');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');
const makeTableMap = require('makeTableMap');
const createQueue = require('createQueue');
const templateStorage = require('templateStorage');

var pixelTracker = null;

const pixelId = data.pixelId;

const eventQueuePush = createQueue('eventQueue');

const getTracker = () => {
  pixelTracker = copyFromWindow('PixelTracker');
  var eventQueue = copyFromWindow('eventQueue');
  pixelTracker.initialize(pixelId);
  if(eventQueue !== null && eventQueue.length) {
    pixelTracker.trackEvent(eventQueue);
    eventQueue.length = 0;
  }
  data.gtmOnSuccess();
};

if (!templateStorage.getItem('alreadyRan')) {
  injectScript(
  'https://web.apploader.in/pixel/v1.0.0/index.iife.js',
  getTracker,
  () => {
    templateStorage.setItem('alreadyRan', false);
    data.gtmOnFailure();
  },
  'credPixel'
);
}

templateStorage.setItem('alreadyRan', true);

const trackPixelEvent = function(event_name, custom_data , user_data) {
  custom_data = custom_data || {};
  user_data = user_data || {};
  const eventDetails = {
    event_name: event_name,
    custom_data: custom_data,
    user_data: user_data
  };
  if(pixelTracker) {
    pixelTracker.trackEvent([eventDetails]);
  } else {
    eventQueuePush(eventDetails);
  }
};

const gtmTagId = data.gtmTagId;
const gtmEventId = data.gtmEventId;
const eventName = data.eventName !== 'Custom' ? data.eventName : data.customEventName;
const customData = data.propertyList ? makeTableMap(data.propertyList, 'name', 'value') : {};
var userData = data.advancedMatchingList ? makeTableMap(data.advancedMatchingList, 'name', 'value') : {};

const eventId = data.eventId;
userData.event_id = eventId;
userData.gtm_tag_id = gtmTagId;
userData.gtm_event_id = gtmEventId;

trackPixelEvent(eventName, customData, userData);

data.gtmOnSuccess();


